<?xml version="1.0" encoding="utf-8"?>
<odoo>
<data noupdate="1">

    <!-- MP Product Packagings -->
    <record id="mp_packaging_DPD" model="stock.package.type">
        <field name="name">MP DPD</field>
        <field name="shipper_package_code">mp_dpd</field>
        <field name="package_carrier_type">mp</field>
    </record>
    <record id="product_product_delivery_mp_dpd" model="product.product">
      <field name="name">MP DPD</field>
      <field name="default_code">Delivery_012</field>
      <field name="type">service</field>
      <field name="categ_id" ref="delivery.product_category_deliveries"/>
      <field name="sale_ok" eval="False"/>
      <field name="purchase_ok" eval="False"/>
      <field name="list_price">0.0</field>
      <field name="invoice_policy">order</field>
    </record>
    <record id="delivery_carrier_mp_dpd" model="delivery.carrier">
      <field name="name">MP DPD</field>
      <field name="product_id" ref="delivery_mp.product_product_delivery_mp_dpd"/>
      <field name="delivery_type">mp</field>
      <field name="mp_username">techuser</field>
      <field name="mp_password">Qwer1234@</field>
      <field name="mp_default_package_type_id" ref="delivery_mp.mp_packaging_DPD"/>
    </record>

    <record id="mp_packaging_SCHENKER" model="stock.package.type">
        <field name="name">MP DB Schenker</field>
        <field name="shipper_package_code">mp_schenker</field>
        <field name="package_carrier_type">mp</field>
    </record>
        <record id="mp_packaging_DHL" model="stock.package.type">
        <field name="name">MP DHL</field>
        <field name="shipper_package_code">mp_dhl</field>
        <field name="package_carrier_type">mp</field>
    </record>
    <record id="mp_packaging_UPS" model="stock.package.type">
        <field name="name">MP UPS</field>
        <field name="shipper_package_code">mp_ups</field>
        <field name="package_carrier_type">mp</field>
    </record>

    <!-- MP Commercial Invoice Sequence -->
    <record id="mp_commercial_invoice_seq" model="ir.sequence">
        <field name="name">MP Commercial Invoice</field>
        <field name="code">delivery_mp.commercial_invoice</field>
        <field name="prefix">CI</field>
        <field name="padding">5</field>
    </record>

</data>
</odoo>
